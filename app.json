{
  "expo": {
    "entryPoint": "./src/index.tsx",
    "name": "Eurofurence",
    "slug": "ef-app-react-native",
    "description": "Your one stop shop to the convention!",
    "owner": "eurofurence",
    "version": "3.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/playstore.png",
    "userInterfaceStyle": "automatic",
    "scheme": "eurofurence",
    "splash": {
      "image": "./assets/images/playstore.png",
      "resizeMode": "contain",
      "backgroundColor": "#006459"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 400,
      "package": "org.eurofurence.connavigator",
      "googleServicesFile": "./android/google-services.json",
      "splash": {
        "resizeMode": "native"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#006459"
      }
    },
    "androidStatusBar": {
      "backgroundColor": "#006459"
    },
    "web": {
      "favicon": "./assets/images/playstore.png"
    },
    "notification": {
      "color": "#006459",
      "icon": "./assets/images/playstore.png"
    },
  }
}