{
  "name": "ef-app_react-native",
  "version": "1.0.0",
  "main": "src/index.tsx",
  "homepage": "https://eurofurence.github.io/ef-app_react-native/",
  "contributors": [
    {
      "name": "requinard"
    },
    {
      "name": "PazuzuEU"
    },
    {
      "name": "Luchs (pinselohrkater)"
    }
  ],
  "convention": {
    "name": "Eurofurence",
    "abbreviation": "EF",
    "identifier": "EF27",
    "appBase": "https://app.eurofurence.org/EF27",
    "apiBase": "https://app.eurofurence.org/EF27/Api",
    "menuColumns": 4,
    "showLogin": false,
    "showServices": false,
    "showCatchEm": false
  },
  "license": "MIT",
  "scripts": {
    "start": " expo start",
    "dev-client": "expo start --dev-client",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "yarn lint:code && yarn lint:translations",
    "lint:code": "eslint ./src --ext js,ts,jsx,tsx",
    "lint:code:fix": "eslint ./src --ext js,ts,jsx,tsx --fix",
    "lint:translations": "eslint --rule 'prettier/prettier: off' --ext json --format node_modules/eslint-plugin-i18n-json/formatter.js ./src/i18n",
    "lint:translations:fix": "eslint --rule 'prettier/prettier: off' --ext json --format node_modules/eslint-plugin-i18n-json/formatter.js ./src/i18n --fix",
    "lint:depcheck": "depcheck",
    "lint:unimported": "npx unimported",
    "test": "jest",
    "eas-build-post-install": "tsc --noEmit && yarn lint:translations && yarn lint:code",
    "bundle:android": "yarn bundle:android:production",
    "bundle:android:development": "eas build --platform android --profile development",
    "bundle:android:preview": "eas build --platform android --profile preview",
    "bundle:android:production": "eas build --platform android --profile production",
    "bundle:web": "expo export:web",
    "submit:android": "eas submit --platform android",
    "submit:web": "gh-pages -d web-build",
    "local:android": "yarn local:android:preview",
    "local:android:development": "npx eas build --platform android --profile development --local",
    "local:android:preview": "npx eas build --platform android --profile preview --local",
    "local:android:production": "npx eas build --platform android --profile production --local",
    "logcat": "adb logcat \\*:S ce.connavigato:\\* ReactNativeJS:\\* -e \"^(?!.*(Invalid ID))\""
  },
  "resolutions": {
    "@types/react": "^18",
    "react-error-overlay": "6.0.9"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.4.7",
    "@hookform/resolvers": "^3.1.1",
    "@openspacelabs/react-native-zoomable-view": "^2.1.1",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-firebase/analytics": "^18.3.0",
    "@react-native-firebase/app": "^18.3.0",
    "@react-native-picker/picker": "2.4.10",
    "@react-navigation/core": "^6.4.9",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "@reduxjs/toolkit": "^1.9.5",
    "@sentry/react-native": "5.5.0",
    "@shopify/flash-list": "1.4.3",
    "expo": "^49.0.6",
    "expo-application": "~5.3.0",
    "expo-av": "~13.4.1",
    "expo-build-properties": "~0.8.3",
    "expo-checkbox": "~2.4.0",
    "expo-constants": "~14.4.2",
    "expo-dev-client": "~2.4.6",
    "expo-device": "~5.4.0",
    "expo-image": "~1.3.2",
    "expo-linking": "~5.0.2",
    "expo-localization": "~14.3.0",
    "expo-notifications": "~0.20.1",
    "expo-status-bar": "~1.6.0",
    "expo-task-manager": "~11.3.0",
    "expo-updates": "~0.18.11",
    "firebase": "^10.1.0",
    "fuse.js": "^6.6.2",
    "i18next": "^23.4.1",
    "intl-pluralrules": "^2.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.2",
    "react-i18next": "^13.0.2",
    "react-native": "0.72.3",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-markdown-display": "^7.0.0-alpha.2",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-star-rating-widget": "^1.7.2",
    "react-native-svg": "13.9.0",
    "react-native-web": "~0.19.6",
    "react-redux": "^8.1.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "sentry-expo": "~7.0.0",
    "ts-pattern": "^5.0.4",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@expo/metro-config": "^0.10.7",
    "@expo/ngrok": "^2.5.0",
    "@expo/webpack-config": "^18.1.2",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.1.3",
    "@types/eslint": "^8",
    "@types/jest": "^29.5.3",
    "@types/lodash": "^4",
    "@types/react": "~18.2.18",
    "@types/react-dom": "~18.0.10",
    "@types/react-test-renderer": "^18",
    "@types/redux-logger": "^3",
    "eslint": "^8.46.0",
    "eslint-config-universe": "^11.3.0",
    "eslint-plugin-i18n-json": "^4.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-testing-library": "^5.11.0",
    "gh-pages": "^5.0.0",
    "jest": "^29.2.1",
    "jest-expo": "^49.0.0",
    "prettier": "^3.0.0",
    "react-native-bundle-visualizer": "^3.1.3",
    "react-test-renderer": "^18.2.0",
    "typescript": "^5.1.3"
  },
  "eslintConfig": {
    "extends": [
      "universe",
      "plugin:i18n-json/recommended"
    ],
    "plugins": [
      "testing-library"
    ],
    "rules": {
      "import/namespace": "off",
      "i18n-json/valid-message-syntax": "off",
      "i18n-json/identical-keys": [
        1,
        {
          "filePath": "../../../../src/i18n/translations.en.json"
        }
      ]
    }
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect",
      "./src/setupTests.js"
    ]
  },
  "prettier": {
    "endOfLine": "crlf"
  },
  "private": true,
  "packageManager": "yarn@3.2.1"
}
